# Graphics
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon0/cable.0/name          u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon0/cable.1/name          u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon0/cable.2/name          u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon2/cable.0/name          u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon2/cable.1/name          u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/88e0000.qcom,msm-eud/extcon/extcon2/cable.2/name          u:object_r:vendor_sysfs_graphics:s0

# Powershare
genfscon sysfs /devices/platform/soc/soc:qcom,pmic_glink/soc:qcom,pmic_glink:qcom,battery_charger/tx_mode    u:object_r:vendor_sysfs_wlc:s0

# Touchscreen
genfscon sysfs /devices/platform/soc/990000.i2c/i2c-0/0-005d/touchscreen        u:object_r:vendor_sysfs_touchpanel:s0
genfscon sysfs /devices/platform/soc/990000.spi/spi_master/spi1/spi1.0/touchscreen u:object_r:vendor_sysfs_touchpanel:s0

# Wakeup
genfscon sysfs /devices/platform/soc/1e00000.qcom,ipa/wakeup/wakeup             u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/soc/984000.i2c/i2c-0/0-005a/wakeup/wakeup      u:object_r:sysfs_wakeup:s0
